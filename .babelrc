{
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "env": {
      "test" : {
        "plugins": ["transform-runtime"]
      },
    "development": {
      "plugins": ["transform-runtime"],
      "presets": [
        "react-hmre"
      ]
    },
    "production": {
      "plugins": [
        "transform-runtime",
        "transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    }
  }
}
